  <ItemGroup>
    <Resource Include="Resources\Banner verde.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Resource>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\Icone cadeado.png">
      <CopyToOutputDirectory></CopyToOutputDirectory>
    </Resource>
